<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>OafLang</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>oaf</AssemblyName>
    <IsPackable>true</IsPackable>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>oaf</ToolCommandName>
    <PackageId>OafLang.Sdk</PackageId>
    <Version>0.1.0</Version>
    <PackageOutputPath>$(MSBuildProjectDirectory)/dist/sdk</PackageOutputPath>
  </PropertyGroup>
</Project>
